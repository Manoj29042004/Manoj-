   "source": [
    "## creating 100 files in a folder named from \"Ashish 1\" to \"Ashish 100\".\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "0f8bbb15",
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(1,101):\n",
    "    name=\"Ashish \"+str(i)+\".txt\"\n",
    "    fd=open(name,'w')\n",
    "    fd.write('file_'+str(i)+'of Ashish sir')\n",
    "    fd.close()"
